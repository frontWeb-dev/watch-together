generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Users {
  id                  Int      @id @default(autoincrement())
  createdDt           DateTime @default(now())
  updatedDt           DateTime @updatedAt
  email               String   @unique
  nickname            String   @unique
  password            String
  cash                BigInt
  reset_passsword_key String?
  roles               String?
  status              String?
}
